{
    "compilerOptions": {
        "jsx": "react-jsx",
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": true,
        "target": "es5",
        "strictNullChecks": true,
        "lib": [
            "es5", "es2015.core", "dom"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "dist",
        "allowJs": true,
        "resolveJsonModule": true
    },
    "include": [
        "src"
    ]
}